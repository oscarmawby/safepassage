    <validationRules>
        <fullName>GCSE_KS4</fullName>
        <active>true</active>
        <description>Ensures grade value for year KS3 is a valid GCSE value</description>
        <errorConditionFormula>AND(

KeyStage__c = 4,

OR( FinalEndScore__c  &lt; 0,FinalEndScore__c &gt; 9, BaselineScore__c  &lt; 0,BaselineScore__c &gt; 9)

)</errorConditionFormula>
        <errorMessage>The grade value is not valid for this Key Stage, GCSE needs to be between 0-9.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PV_questions_0_10</fullName>
        <active>false</active>
        <description>Oscar Mawby @ Impact Box 12/12/19</description>
        <errorConditionFormula>OR(

 PostPVEnjoyedTuition__c &lt;= 10,
 PostPVHelpedToImprove__c &lt;= 10,
 PrePVAbilityPerception__c &lt;= 10,
 PrePVAskingForHelp__c &lt;= 10,
 PrePVEnjoyment__c &lt;= 10,
 PrePVResilience__c &lt;= 10
)</errorConditionFormula>
        <errorMessage>PV Values must be between 0-10</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PuMA_PiRA_KS3</fullName>
        <active>true</active>
        <description>Ensures grade value for year 7, 8 and 9 is a valid PuMA/PiRA value</description>
        <errorConditionFormula>AND(

 KeyStage__c = 3,

OR( FinalEndScore__c  &lt; 70,FinalEndScore__c &gt; 130, BaselineScore__c  &lt; 70,BaselineScore__c &gt; 130)

)</errorConditionFormula>
        <errorMessage>The grade value is not valid for KS3, PuMA/PiRA needs to be between 70-130.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SATs_KS2</fullName>
        <active>true</active>
        <description>Ensures grade value for year KS2 is a valid SATs value</description>
        <errorConditionFormula>AND(

 KeyStage__c = 2,

OR( FinalEndScore__c  &lt; 80,FinalEndScore__c &gt; 120, BaselineScore__c  &lt; 80,BaselineScore__c &gt; 120)

)</errorConditionFormula>
        <errorMessage>The grade value is not valid for this Key Stage, SATs need to be between 80-120.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tutor_Is_Changed_Confirmation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED( PupilsTutor__c), NOT(ISBLANK(PRIORVALUE(PupilsTutor__c))),  ConfirmTutorChange__c = FALSE)</errorConditionFormula>
        <errorMessage>If you wish to change the pupil&apos;s tutor and update all future attendance records, you must confirm this change by ticking the &apos;Confirm Tutor Change&apos; field</errorMessage>
    </validationRules>
