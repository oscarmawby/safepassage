    <fields>
        <fullName>SessionLengthHours__c</fullName>
        <description>Oscar Mawby @ Impact Box 30/12/19</description>
        <externalId>false</externalId>
        <formula>(Hour(  TIMEVALUE(EndDateTime__c)  ) - Hour(  TIMEVALUE(StartDateTime__c)  )) + ((Minute(TIMEVALUE(EndDateTime__c)) - Minute(TIMEVALUE(StartDateTime__c)))/60)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Calculated as a decimal e.g. 1.5 hours = 1 hour, 30 minutes</inlineHelpText>
        <label>Session Length (hours)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
