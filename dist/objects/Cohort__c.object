        <fullName>KeyStage__c</fullName>
        <externalId>false</externalId>
        <formula>IF(

 OR( ISPICKVAL( SchoolYear__c , &apos;5&apos;), ISPICKVAL( SchoolYear__c , &apos;6&apos;)), &apos;KS2&apos;,

IF(

 OR( ISPICKVAL( SchoolYear__c , &apos;7&apos;), ISPICKVAL( SchoolYear__c , &apos;8&apos;),ISPICKVAL( SchoolYear__c , &apos;9&apos;)), &apos;KS3&apos;,

IF(

 OR( ISPICKVAL( SchoolYear__c , &apos;10&apos;), ISPICKVAL( SchoolYear__c , &apos;11&apos;)), &apos;KS4&apos;,

&apos;INVALID SCHOOL YEAR&apos;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Key Stage</label>
        <required>false</required>
