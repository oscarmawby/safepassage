    <validationRules>
        <fullName>NonContact_Amount_Populated</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND( ISBLANK(NonContactPay__c)=TRUE, ISBLANK(NonContactTimehours__c)=FALSE), AND(ISBLANK(NonContactPay__c)= FALSE, ISBLANK(NonContactTimehours__c)=TRUE))</errorConditionFormula>
        <errorMessage>Please specify the non-contact hours and pay amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequiredFieldsForOpenStatus</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISPICKVAL( Status__c, &apos;Open&apos;),

OR(

ISBLANK( EndDate__c ), ISBLANK(TEXT( DayofWeek__c )), ISBLANK( ContactTimePay__c ), ISBLANK( Cohort__c ), ISBLANK(  EndTime__c  ), ISBLANK(TEXT(  Frequency__c  )), ISBLANK(  StartDate__c ), ISBLANK(  StartTime__c  ), ISBLANK(TEXT( TutorYPRatio__c  )))

