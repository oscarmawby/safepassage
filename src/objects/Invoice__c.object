    <fields>
        <fullName>DaysOverdue__c</fullName>
        <description>Created by Oscar @ Impact Box, 30/07/2020</description>
        <externalId>false</externalId>
        <formula>IF( AND(ScheduledDate__c &gt; TODAY(),ISBLANK( InvoicePaidDate__c )),NULL,

IF(ISBLANK( InvoicePaidDate__c ),

TODAY()- ScheduledDate__c ,

InvoicePaidDate__c - ScheduledDate__c)

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This will count the days an invoice has gone unpaid after scheduled date. Once paid it will will show the number of days between the scheduled date and the paid date.</inlineHelpText>
        <label>Days Overdue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
